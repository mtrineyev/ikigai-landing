{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/submitForm",
        "function": "submitForm"
      }
    ]
  },
  "functions": {
    "source": "functions",
    "runtime": "nodejs20",
    "region": "europe-west4"
  }
}
